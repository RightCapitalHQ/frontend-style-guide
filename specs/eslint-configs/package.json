{
  "name": "@rightcapital/style-guide-spec-eslint-configs",
  "private": true,
  "scripts": {
    "fix": "eslint --fix . && prettier --write .",
    "lint": "concurrently pnpm:lint:*",
    "lint:eslint": "eslint --report-unused-disable-directives --max-warnings=0 .",
    "lint:prettier-eslint": "prettier -c .",
    "test": "vitest"
  },
  "dependencies": {
    "react": "18.3.1",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@rightcapital/eslint-config": "workspace:*",
    "@rightcapital/prettier-config": "workspace:*",
    "@types/react": "18.3.7",
    "concurrently": "9.0.1",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "execa": "9.4.0",
    "prettier": "3.3.3",
    "vitest": "2.1.1"
  }
}
